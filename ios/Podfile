# Uncomment the next line to define a global platform for your project
platform :ios, '10.0'

target 'WelaaaV2' do
  # Uncomment the next line if you're using Swift or would like to use dynamic frameworks
  # use_frameworks!

  # Pods for WelaaaV2
  
  # Firebase Analytics + Firebase Cloud Message
  pod 'Firebase/Core'
  pod 'Firebase/Messaging'
  
  # Fabric
  pod 'Fabric'
  pod 'Crashlytics'
  
  # Facebook SDKs
  pod 'Bolts'
  pod 'FBSDKCoreKit'
  pod 'FBSDKShareKit'
  pod 'FBSDKLoginKit'

  # KakaoOpenSDK
  pod 'KakaoOpenSDK'

  # AFNetworking
  pod 'AFNetworking', '~> 3.0'
  
  # KeyChain
  pod 'SAMKeychain'
  
  # Toast message
  pod 'Toast', '~> 4.0.0'

  # Showing images from web
  pod 'SDWebImage', '~> 4.0'

 #pod 'RNDeviceInfo', :path => '../node_modules/react-native-device-info'

end
use_frameworks!
